#!/bin/bash

. ../git_utils.sh

setup_task "1234" "minor"

create_commit
create_commit

close-task.sh

test ! -f ".CI_TASKID"
test ! -f ".CI_NEXTRELEASE"
test "$(git rev-parse master^2)" = "$(git rev-parse feature/1234)"
test "$(git log -1 --format="%s")" = "rif. #1234:minor:Merged branch feature/1234"